<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.meituan.product.mapper.CompanyInfoMapper">
    
    <!-- 获取公司信息 -->
    <select id="getCompanyInfo" resultType="com.meituan.product.entity.CompanyInfo">
        SELECT * FROM t_company_info
        WHERE id = 1
        LIMIT 1
    </select>
    
</mapper>
